<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="750.0" prefWidth="1140.0" spacing="10.0" styleClass="mainFxmlClass" stylesheets="@ajouteroffre.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.RecettesViewController">
   <children>
      
       
      <Label maxHeight="-Infinity" prefHeight="24.0" prefWidth="128.0" styleClass="fontsFxmlClass" stylesheets="@ajouteroffre.css" text="ALL Recettes">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <font>
            <Font name="Century Schoolbook" size="19.0" />
         </font>
      </Label>
      <Button alignment="TOP_CENTER" depthTest="DISABLE" mnemonicParsing="false" onAction="#goToAddRecetteForm" styleClass="myButton" stylesheets="@ajouteroffre.css" text="ADD NEW Recette" textAlignment="CENTER" />
      <Label fx:id="RecetteNumberLabel" maxHeight="-Infinity" styleClass="fontsFxmlClass">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <font>
            <Font name="Century Schoolbook" size="19.0" />
         </font>
      </Label>
      <ScrollPane prefHeight="393.0" prefWidth="1140.0">
         <content>
            <HBox fx:id="allRecettesLayout" maxHeight="-Infinity" prefHeight="421.0" prefWidth="1140.0" styleClass="CARD" stylesheets="@ajouteroffre.css" />
         </content>
      </ScrollPane>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
